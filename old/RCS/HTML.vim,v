d1019 2
d1026 12
a1037 6
	nnoremap <silent> <buffer> q <C-w>c
	nnoremap <silent> <buffer> <space> <C-f>
	nnoremap <silent> <buffer> b <C-b>
  nnoremap <silent> <buffer> <tab> :call search('\w\+ = #\x\{6\}')<CR>
  execute 'nnoremap <silent> <buffer> <cr> :call <SID>ColorSelect(' . curbuf . ')<CR>'
  execute 'nnoremap <silent> <buffer> <2-leftmouse> :call <SID>ColorSelect(' . curbuf . ')<CR>'
