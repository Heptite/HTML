d1965 7
a1971 7
  command! -nargs=+ HTMLmenu call s:HTMLleadmenu(<f-args>)
  function! s:HTMLleadmenu(type, level, name, item, ...)
    if a:0 == 1
      let pre = a:1
    else
      let pre = ''
    endif
d1973 5
a1977 5
    if a:level == '-'
      let level = ''
    else
      let level = a:level
    endif
d1979 1
a1979 1
    let name = escape(a:name, ' ')
d1981 3
a1983 3
    execute a:type . ' ' . level . ' ' . name . '<tab>' . g:html_map_leader . a:item
      \ . ' ' . pre . g:html_map_leader . a:item
  endfunction
d2253 29
a2281 3
