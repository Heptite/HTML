  execute "normal 1G0i<HTML>\<CR> <HEAD>\<CR>\<CR>  <TITLE></TITLE>\<CR>  <BASE HREF=\"\">\<ESC>"
  execute "normal o  <META NAME=\"Generator\" CONTENT=\"vim (Vi IMproved editor; http://www.vim.org/)\">\<ESC>"
  execute "normal o  <META NAME=\"Author\" CONTENT=\"" . g:html_authorname . "\">\<ESC>"
  execute "normal o  <META NAME=\"Copyright\" CONTENT=\"Copyright (C) " . strftime("%B %d, %Y") . ' ' . g:html_authorname . "\">\<ESC>"
  execute "normal o  <LINK REV=\"made\" HREF=\"mailto:" . g:html_authoremail . "\">\<CR>\<CR> </HEAD>\<ESC>"
  execute "normal o <BODY BGCOLOR=\"" . g:html_bgcolor . "\"\<ESC>"
  execute "normal A TEXT=\"" . g:html_textcolor . "\"\<ESC>"
  execute "normal A LINK=\"" . g:html_linkcolor . "\"\<ESC>"
  execute "normal A ALINK=\"" . g:html_alinkcolor . "\"\<ESC>"
  execute "normal A VLINK=\"" . g:html_vlinkcolor . "\">\<CR>\<ESC>"
  execute "normal o  <H1 ALIGN=CENTER></H1>\<CR>\<CR>  <P></P>\<CR>\<ESC>"
  execute "normal o  <HR WIDTH=\"75%\">\<CR>\<CR>  <P>\<CR>   Last Modified:\<CR>   <I>\<ESC>"
  execute "normal o    <!-- Last modification date: -->\<CR>    " . strftime("%B %d, %Y") . "\<CR>\<ESC>"
  execute "normal o   </I>\<CR>  </P>\<CR>\<CR>  <ADDRESS>\<ESC>"
