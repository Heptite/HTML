" $Id: HTML.vim,v 1.185 2008/05/09 18:16:19 infynity Exp infynity $
d961 1
a961 1
function! s:ShowColors()
d1034 11
a1044 4
  execute 'noremap <silent> <buffer> <cr> :call <SID>ColorSelect(' . curbuf . ')<CR>'
  execute 'inoremap <silent> <buffer> <cr> <C-o>:call <SID>ColorSelect(' . curbuf . ')<CR>'
  execute 'noremap <silent> <buffer> <2-leftmouse> :call <SID>ColorSelect(' . curbuf . ')<CR>'
  execute 'inoremap <silent> <buffer> <2-leftmouse> <C-o>:call <SID>ColorSelect(' . curbuf . ')<CR>'
d1047 1
a1047 1
function! s:ColorSelect(bufnr)
d1065 8
a1072 1
  exe 'normal i' . colora[1] . "\<esc>"
d2592 7
a2598 2
  command! ColorSelect call s:ShowColors()
	amenu <silent> HTML.Colors.Display\ All :call <SID>ShowColors()<CR>
@


